{
  "name": "next-js-boilerplate",
  "version": "3.61.0",
  "author": "Nick",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next eslint .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "check-types": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit --pretty",
    "commit": "cz",
    "prepare": "husky",
    "clean": "rimraf .next out",
    "sprite:build": "node ./sprite/sprite.mjs --input-type=module",
    "sync-media": "node scripts/syncMedia.mjs"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.16",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/lab": "^6.0.0-beta.26",
    "@mui/material": "^6.4.4",
    "@payloadcms/db-postgres": "^3.22.0",
    "@payloadcms/live-preview-react": "^3.22.0",
    "@payloadcms/next": "^3.22.0",
    "@payloadcms/payload-cloud": "^3.22.0",
    "@payloadcms/plugin-seo": "^3.22.0",
    "@payloadcms/richtext-lexical": "^3.22.0",
    "@payloadcms/storage-s3": "^3.23.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/nextjs": "^8.51.0",
    "@spotlightjs/spotlight": "^2.10.2",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tabler/icons-react": "^3.30.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "escape-html": "^1.0.3",
    "framer-motion": "^12.4.2",
    "fs": "^0.0.1-security",
    "graphql": "^16.10.0",
    "lexical": "0.21.0",
    "lucide-react": "^0.475.0",
    "motion": "^12.4.1",
    "next": "^15.1.6",
    "next-intl": "^3.26.3",
    "next-themes": "^0.4.4",
    "payload": "^3.22.0",
    "pg": "^8.13.1",
    "pretty": "^2.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-hook-form": "^7.54.2",
    "react-responsive": "^10.0.0",
    "react-responsive-masonry": "^2.7.1",
    "resend": "^4.1.2",
    "sonner": "^1.7.4",
    "svg-sprite": "^2.0.4",
    "swiper": "^11.2.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint-react/eslint-plugin": "^1.24.1",
    "@faker-js/faker": "^9.4.0",
    "@next/bundle-analyzer": "^15.1.6",
    "@next/eslint-plugin-next": "^15.1.6",
    "@playwright/test": "^1.50.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "^22.10.10",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.8",
    "@types/react-responsive-masonry": "^2.6.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/expect": "^3.0.4",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.2",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.1",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4",
    "vitest-fail-on-console": "^0.7.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
